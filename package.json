{
    "name": "blackguard-timer-suite",
    "version": "2.0.0",
    "description": "A suite of timers used for Maplestory",
    "main": "dist/main/index.js",
    "scripts": {
        "watch:main": "rimraf ./dist/main && cross-env NODE_ENV=development webpack --watch --config ./main.webpack.config.js",
        "watch:renderer": "rimraf ./dist/renderer && cross-env NODE_ENV=development webpack --watch --config ./renderer.webpack.config.js",
        "compile:main": "rimraf ./dist/main && cross-env NODE_ENV=production webpack --config ./main.webpack.config.js",
        "compile:renderer": "rimraf ./dist/renderer && cross-env NODE_ENV=production webpack --config ./renderer.webpack.config.js",
        "start": "electron-forge start -- --with-dev-tools",
        "make": "npm run compile:main && npm run compile:renderer && electron-forge make"
    },
    "author": "Mnesma",
    "license": "ISC",
    "devDependencies": {
        "@electron-forge/cli": "7.4.0",
        "@electron-forge/maker-deb": "7.4.0",
        "@electron-forge/maker-rpm": "7.4.0",
        "@electron-forge/maker-squirrel": "7.4.0",
        "@electron-forge/maker-zip": "7.4.0",
        "@electron-forge/plugin-auto-unpack-natives": "7.4.0",
        "@types/electron-squirrel-startup": "1.0.2",
        "@types/node": "20.14.11",
        "cross-env": "7.0.3",
        "css-loader": "7.1.2",
        "electron": "31.2.1",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.6.0",
        "image-minimizer-webpack-plugin": "4.0.2",
        "mini-css-extract-plugin": "2.9.0",
        "rimraf": "6.0.1",
        "ts-loader": "9.5.1",
        "webpack": "5.93.0",
        "webpack-cli": "5.1.4"
    },
    "dependencies": {
        "electron-squirrel-startup": "1.0.1",
        "leac": "0.6.0",
        "node-global-key-listener": "0.1.1",
        "peberminta": "0.9.0",
        "typescript": "5.5.3"
    }
}
